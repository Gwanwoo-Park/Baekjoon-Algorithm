#define _USE_MATH_DEFINES
#include <iostream>
#include <string.h>
#include <string>
#include <algorithm>
#include <math.h>
#include <cmath>
#include <stdio.h>
#include <cstdlib>
using namespace std;

int main() {
	int t;

	cin >> t;

	int customer = 1;

	for(int i = 0; i < t; i++) {
		int n;

		cin >> n;

		cin.ignore();

		string *s = new string[n];
		string *tmp = new string[n];

		for(int j = 0; j < n; j++) {
			getline(cin, tmp[j]);
			s[j] = tmp[j];
		}

		for(int j = 0; j < n; j++) {
			int len = tmp[j].length();

			for(int k = 0, ind = 0; k < len; k++) {
				if(tmp[j][k] != ' ') {
					s[j][ind++] = tmp[j][k];
				}
			}
		}

		cin >> n;

		for(int j = 0; j < n; j++) {
			int num, first, second;
			char a, b;

			if(j == 0) {
				cout << "Customer " << customer << '\n';
				customer++;
			}

			cin >> num >> first >> second >> a >> b;

			num--;
			first--;
			second--;

			if(s[num][first] == a && s[num][second] == b) {
				cout << "correct\n";
			} else {
				cout << "error\n";
			}
		}
	}
}
